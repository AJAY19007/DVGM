<h1>Edit Grade</h1>

<%= form_for @grade do |f| %>
  <%= render 'shared/errors', object: @grade %>
  <div>
    <%= f.label :lecture %>
    <%= @grade.lecture.name %>
  </div>

  <div>
    <%= f.label :student %>
    <%= @grade.student.login %>
  </div>

  <div>
    <%= f.label :grade %>
    <%= f.number_field :grade %>
  </div>
  <%= f.submit %>
<% end %>
